{
  "name": "Overseer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "gulp build",
    "dev": "SET NODE_ENV=development && nodemon --watch dist --ignore dist/public/js/main.js dist/app.js",
    "inspect": "node --inspect dist/app.js",
    "start": "export NODE_ENV=production && sudo -E forever start dist/app.js",
    "stop": "sudo forever stop dist/app.js",
    "deploy": "npm stop && git pull && npm run build && npm start"
  },
  "dependencies": {
    "axios": "^0.15.1",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-session": "^1.15.3",
    "express-socket.io-session": "^1.3.2",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "node-hue-api": "^2.2.0",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1",
    "request": "^2.75.0",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.4.6"
  },
  "devDependencies": {
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-es2015-destructuring": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-connect": "^5.0.0",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^2.1.1",
    "gulp-svgmin": "^1.2.3",
    "gulp-webpack": "^1.5.0",
    "sass-material-colors": "0.0.5",
    "source-map-loader": "^0.1.5"
  }
}
