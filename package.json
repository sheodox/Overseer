{
  "name": "Overseer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "gulp build",
    "dev": "NODE_ENV=development && nodemon --watch dist --ignore dist/public/js/main.js --inspect dist/app.js",
    "inspect": "node --inspect dist/app.js",
    "start": "export NODE_ENV=production && forever start dist/app.js",
    "stop": "forever stop dist/app.js",
    "deploy": "npm stop && git pull && npm run build && npm start",
    "test": "export NODE_ENV=production && node dist/app.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "express-session": "^1.15.3",
    "express-socket.io-session": "^1.3.2",
    "morgan": "^1.9.0",
    "node-hue-api": "^2.2.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "pug": "^2.0.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.1.1",
    "request": "^2.75.0",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.1.1",
    "sqlite3": "^4.0.0",
    "three": "^0.96.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^8.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-connect": "^5.7.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svgmin": "^2.2.0",
    "gulp-webpack": "^1.5.0",
    "sass-material-colors": "0.0.5",
    "source-map-loader": "^0.2.4"
  }
}
