{
  "name": "overseer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prod-build": "export NODE_ENV=production && webpack",
    "dev-build": "export NODE_ENV=development && webpack",
    "dev": "NODE_ENV=development && nodemon --watch dist --ignore dist/public/js/main.js --inspect dist/app.js",
    "inspect": "node --inspect dist/app.js",
    "start": "export NODE_ENV=production && forever start dist/app.js",
    "stop": "forever stop dist/app.js",
    "deploy": "npm stop && git pull && npm install && npm run prod-build && npm start",
    "test": "export NODE_ENV=production && node dist/app.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "express-session": "^1.15.3",
    "express-socket.io-session": "^1.3.2",
    "morgan": "^1.9.0",
    "node-hue-api": "^2.2.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "pug": "^2.0.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^4.1.1",
    "request": "^2.75.0",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.5.0",
    "sharp": "^0.24.0",
    "socket.io": "^2.1.1",
    "sqlite3": "^4.0.0",
    "three": "^0.96.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "babelify": "^8.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "file-loader": "^5.0.2",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.0",
    "sass-material-colors": "0.0.5",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
